# API Configuration
API_KEY=your_secret_api_key_here

# LLM Provider Configuration (Choose one)
# Options: groq (recommended - free & fast), openrouter, openai, local
LLM_PROVIDER=groq

# Groq Cloud API (FREE - Recommended)
# Get your free API key at: https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# OpenRouter API (FREE tier available)
# Get your free API key at: https://openrouter.ai
# OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenAI API (Paid)
# OPENAI_API_KEY=your_openai_api_key_here

# Evaluation Endpoint
EVALUATION_ENDPOINT=https://hackathon.guvi.in/api/updateHoneyPotFinalResult

# Optional: Use local LLM instead (Ollama)
# USE_LOCAL_LLM=false
# LOCAL_LLM_URL=http://localhost:11434
